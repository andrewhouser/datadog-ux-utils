{
  "$schema": "https://typedoc.org/schema.json",
  "name": "datadog-ux-utils Documentation",
  // Expand every file under src so module pages reflect folder structure
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "./README.md",
  "cleanOutputDir": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Config",
    "API",
    "Performance",
    "React",
    "Telemetry",
    "UX",
    "Other"
  ]
}
