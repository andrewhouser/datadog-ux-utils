{
  "name": "datadog-ux-utils",
  "version": "1.0.1",
  "description": "Datadog RUM focused UX & performance toolkit: API guards (retry, breaker, rate), React telemetry (error boundary, profiler, Suspense), web vitals & resource observers, offline queues.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./perf": {
      "types": "./dist/perf/index.d.ts",
      "import": "./dist/perf.mjs",
      "require": "./dist/perf.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api.mjs",
      "require": "./dist/api.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "import": "./dist/env.mjs",
      "require": "./dist/env.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry/index.d.ts",
      "import": "./dist/telemetry.mjs",
      "require": "./dist/telemetry.js"
    },
    "./ux": {
      "types": "./dist/ux/index.d.ts",
      "import": "./dist/ux.mjs",
      "require": "./dist/ux.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewhouser/datadog-ux-utils.git"
  },
  "homepage": "https://andrewhouser.github.io/datadog-ux-utils/",
  "bugs": {
    "url": "https://github.com/andrewhouser/datadog-ux-utils/issues"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage --coverage.threshold.lines=80 --coverage.threshold.functions=75 --coverage.threshold.branches=65 --coverage.threshold.statements=80",
    "test:coverage": "vitest run --coverage --coverage.threshold.lines=80 --coverage.threshold.functions=75 --coverage.threshold.branches=65 --coverage.threshold.statements=80",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build && npm run docs",
    "docs": "typedoc --skipErrorChecking",
    "docs:serve": "typedoc --skipErrorChecking --watch",
    "docs:dev": "typedoc --skipErrorChecking --watch --preserveWatchOutput",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "semantic-release"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "20 KB"
    },
    {
      "path": "dist/react.mjs",
      "limit": "6 KB"
    },
    {
      "path": "dist/perf.mjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/api.mjs",
      "limit": "8 KB"
    },
    {
      "path": "dist/errors.mjs",
      "limit": "6 KB"
    },
    {
      "path": "dist/telemetry.mjs",
      "limit": "6 KB"
    },
    {
      "path": "dist/ux.mjs",
      "limit": "6 KB"
    },
    {
      "path": "./size-limit/only-ddFetch.js",
      "limit": "3 KB"
    },
    {
      "path": "./size-limit/only-registerWebVitals.js",
      "limit": "4 KB"
    },
    {
      "path": "./size-limit/only-ErrorBoundary.js",
      "limit": "4 KB"
    },
    {
      "path": "./size-limit/only-componentTelemetry.js",
      "limit": "2 KB"
    }
  ],
  "keywords": [
    "datadog",
    "datadog-rum",
    "performance",
    "ux",
    "monitoring",
    "telemetry",
    "web-vitals",
    "react",
    "circuit-breaker",
    "retry",
    "rate-limit",
    "typescript",
    "tree-shakeable",
    "esm",
    "commonjs"
  ],
  "author": {
    "name": "Andrew Houser",
    "email": "houserdesign@gmail.com"
  },
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18 <21",
    "react-dom": ">=18 <21",
    "@datadog/browser-rum": ">=6.0.0",
    "@datadog/browser-logs": ">=6.0.0",
    "web-vitals": ">=3 <6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "web-vitals": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@types/react-dom": "^19.1.7",
    "@types/react": "^19.1.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "jsdom": "^26.1.0",
    "typedoc-plugin-markdown": "^4.0.3",
    "typedoc": "^0.28.10",
    "typescript": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vite": "^5.1.4",
    "vitest": "^1.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@datadog/browser-rum": "^6.18.0",
    "@datadog/browser-logs": "^6.18.0",
    "web-vitals": "^5.1.0",
    "size-limit": "^11.1.4",
    "@size-limit/preset-small-lib": "^11.1.4",
    "semantic-release": "^23.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^10.0.0",
    "@semantic-release/npm": "^11.0.0"
  }
}
